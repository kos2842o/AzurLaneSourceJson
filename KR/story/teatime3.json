    {
      "actor": 205010,
      "side": 0,
      "dir": 1,
      "nameColor": "#92fc63",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "……그래? ……그럼…… 쿠울 쿠울……"

    {
      "actor": 502020,
      "side": 0,
      "dir": 1,
      "nameColor": "#92fc63",
      "typewriter": {
        "speed": 0.05,
        "speedUp": 0.01
      },
      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "say": "무슨 꿍꿍이야!"

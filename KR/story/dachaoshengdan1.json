      "painting": {
        "alpha": 0.3,
        "time": 1
      },
      "bgName": "bg_story_room"
    },
    {
      "say": "어떻게 하면 좋지. 에잇……",
      "actor": 205020,
      "side": 2,
      "dir": 1,
      "nameColor": "#92fc63",
      "typewriter": {
